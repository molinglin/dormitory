<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zust.se.dao.StuApplicationDao">
    <insert id="insert" parameterType="StuApplication" keyProperty="id" useGeneratedKeys="true">
        insert into stuapplication(uid1,uid2,applicationid,access) values (#{uid1},#{uid2},#{applicationid},#{access})
    </insert>
</mapper>